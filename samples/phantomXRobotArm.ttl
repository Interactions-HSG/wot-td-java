@prefix dct: <http://purl.org/dc/terms/> .
@prefix onto: <https://ci.mines-stetienne.fr/kg/ontology#> .
@prefix hctl: <https://www.w3.org/2019/wot/hypermedia#> .
@prefix htv: <http://www.w3.org/2011/http#> .
@prefix js: <https://www.w3.org/2019/wot/json-schema#> .
@prefix td: <https://www.w3.org/2019/wot/td#> .
@prefix wotsec: <https://www.w3.org/2019/wot/security#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://api.interactions.ics.unisg.ch/v1.2/leubot> a onto:PhantomX, td:Thing;
  dct:title "leubot1";
  td:hasSecurityConfiguration [ a wotsec:APIKeySecurityScheme ;
      wotsec:in "header" ;
      wotsec:name "X-API-Key" ;
  ] ;
td:hasPropertyAffordance [
        a td:PropertyAffordance, js:ObjectSchema, onto:Posture ;
    js:properties [ a js:IntegerSchema, onto:BaseJointValue;
              js:propertyName "Base"
            ],
      [ a js:IntegerSchema, onto:ShoulderJointValue;
              js:propertyName "Shoulder"
            ],
      [ a js:IntegerSchema, onto:ElbowJointValue;
              js:propertyName "Elbow"
            ],
      [ a js:IntegerSchema, onto:WristAngleJointValue;
              js:propertyName "WristAngle"
            ],
      [ a js:IntegerSchema, onto:WristRotationJointValue;
              js:propertyName "WristRotation"
            ],
      [ a js:IntegerSchema, onto:GripperJointValue;
              js:propertyName "Gripper"
            ] ;
        td:hasForm [
            hctl:hasTarget <https://api.interactions.ics.unisg.ch/leubot1/v1.2/posture> ;
        ] ;
    ] ;
td:hasPropertyAffordance [
        a td:PropertyAffordance, js:ObjectSchema, onto:Gripper ;
    js:properties [ a js:StringSchema, onto:JointName;
              js:propertyName "name"
            ],
      [ a js:IntegerSchema, onto:JointValue;
              js:propertyName "value"
            ] ;
        td:hasForm [
            hctl:hasTarget <https://api.interactions.ics.unisg.ch/leubot1/v1.2/gripper> ;
        ] ;
    ] ;
td:hasPropertyAffordance [
        a td:PropertyAffordance, js:ObjectSchema, onto:Elbow ;
    js:properties [ a js:StringSchema, onto:JointName;
              js:propertyName "name"
            ],
      [ a js:IntegerSchema, onto:JointValue;
              js:propertyName "value"
            ] ;
        td:hasForm [
            hctl:hasTarget <https://api.interactions.ics.unisg.ch/leubot1/v1.2/elbow> ;
        ] ;
    ] ;
td:hasPropertyAffordance [
        a td:PropertyAffordance, js:ObjectSchema, onto:Base ;
    js:properties [ a js:StringSchema, onto:JointName;
              js:propertyName "name"
            ],
      [ a js:IntegerSchema, onto:JointValue;
              js:propertyName "value"
            ] ;
        td:hasForm [
            hctl:hasTarget <https://api.interactions.ics.unisg.ch/leubot1/v1.2/base> ;
        ] ;
    ] ;
td:hasPropertyAffordance [
        a td:PropertyAffordance, js:ObjectSchema, onto:WristAngle ;
    js:properties [ a js:StringSchema, onto:JointName;
              js:propertyName "name"
            ],
      [ a js:IntegerSchema, onto:JointValue;
              js:propertyName "value"
            ] ;
        td:hasForm [
            hctl:hasTarget <https://api.interactions.ics.unisg.ch/leubot1/v1.2/wrist/angle> ;
        ] ;
    ] ;
td:hasPropertyAffordance [
        a td:PropertyAffordance, js:ObjectSchema, onto:WristRotation ;
    js:properties [ a js:StringSchema, onto:JointName;
              js:propertyName "name"
            ],
      [ a js:IntegerSchema, onto:JointValue;
              js:propertyName "value"
            ] ;
        td:hasForm [
            hctl:hasTarget <https://api.interactions.ics.unisg.ch/leubot1/v1.2/wrist/rotation> ;
        ] ;
    ] ;
td:hasPropertyAffordance [
        a td:PropertyAffordance, js:ObjectSchema, onto:Shoulder ;
    js:properties [ a js:StringSchema, onto:JointName;
              js:propertyName "name"
            ],
      [ a js:IntegerSchema, onto:JointValue;
              js:propertyName "value"
            ] ;
        td:hasForm [
            hctl:hasTarget <https://api.interactions.ics.unisg.ch/leubot1/v1.2/shoulder> ;
        ] ;
    ] ;
td:hasActionAffordance [ a onto:Reset, td:ActionAffordance;
      dct:title "Reset";
      td:hasForm [
          htv:methodName "PUT";
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction;
          hctl:hasTarget <https://api.interactions.ics.unisg.ch/leubot1/v1.2/reset>
        ];
    ], [ a onto:SetElbow, td:ActionAffordance;
      dct:title "Set Elbow";
      td:hasForm [
          htv:methodName "PUT";
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction;
          hctl:hasTarget <https://api.interactions.ics.unisg.ch/leubot1/v1.2/elbow>
        ];
      td:hasInputSchema [ a onto:ElbowJoint, js:ObjectSchema;
          js:properties [ a js:IntegerSchema;
              js:maximum "900"^^xsd:int;
              js:minimum "210"^^xsd:int;
              js:propertyName "value"
            ];
          js:required "value"
        ]
    ], [ a onto:SetWristAngle, td:ActionAffordance;
      dct:title "Set Wrist Angle";
      td:hasForm [
          htv:methodName "PUT";
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction;
          hctl:hasTarget <https://api.interactions.ics.unisg.ch/leubot1/v1.2/wrist/angle>
        ];
      td:hasInputSchema [ a onto:WristAngleJoint, js:ObjectSchema;
          js:properties [ a js:IntegerSchema;
              js:maximum "830"^^xsd:int;
              js:minimum "200"^^xsd:int;
              js:propertyName "value"
            ];
          js:required "value"
        ]
    ], [ a onto:SetWristRotation, td:ActionAffordance;
      dct:title "Set Wrist Rotation";
      td:hasForm [
          htv:methodName "PUT";
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction;
          hctl:hasTarget <https://api.interactions.ics.unisg.ch/leubot1/v1.2/wrist/rotation>
        ];
      td:hasInputSchema [ a onto:WristRotateJoint, js:ObjectSchema;
          js:properties [ a js:IntegerSchema;
              js:maximum "1023"^^xsd:int;
              js:minimum "0"^^xsd:int;
              js:propertyName "value"
            ];
          js:required "value"
        ]
    ], [ a onto:SetBase, td:ActionAffordance;
      dct:title "Set Base";
      td:hasForm [
          htv:methodName "PUT";
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction;
          hctl:hasTarget <https://api.interactions.ics.unisg.ch/leubot1/v1.2/base>
        ];
      td:hasInputSchema [ a onto:BaseJoint, js:ObjectSchema;
          js:properties [ a js:IntegerSchema;
              js:maximum "1023"^^xsd:int;
              js:minimum "512"^^xsd:int;
              js:propertyName "value"
            ];
          js:required "value"
        ]
    ], [ a onto:SetGripper, td:ActionAffordance;
      dct:title "Set Gripper";
      td:hasForm [
          htv:methodName "PUT";
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction;
          hctl:hasTarget <https://api.interactions.ics.unisg.ch/leubot1/v1.2/gripper>
        ];
      td:hasInputSchema [ a onto:GripperJoint, js:ObjectSchema;
          js:properties [ a js:IntegerSchema;
              js:maximum "512"^^xsd:int;
              js:minimum "0"^^xsd:int;
              js:propertyName "value"
            ];
          js:required "value"
        ]
    ], [ a onto:SetShoulder, td:ActionAffordance;
      dct:title "Set Shoulder";
      td:hasForm [
          htv:methodName "PUT";
          hctl:forContentType "application/json";
          hctl:hasOperationType td:invokeAction;
          hctl:hasTarget <https://api.interactions.ics.unisg.ch/leubot1/v1.2/shoulder>
        ];
      td:hasInputSchema [ a onto:ShoulderJoint, js:ObjectSchema;
          js:properties [ a js:IntegerSchema;
              js:maximum "810"^^xsd:int;
              js:minimum "205"^^xsd:int;
              js:propertyName "value"
            ];
          js:required "value"
        ]
    ].
